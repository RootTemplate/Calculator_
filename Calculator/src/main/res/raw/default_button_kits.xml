<?xml version="1.0" encoding="utf-8"?>
<ButtonKits isDefault="true">
    <Buttons>
        <!--
            Describes all buttons used in <Button> in <Kits>.
            Attributes:
                ID. The unique identifier of the button.
                NAME. [Optional] The visible name of the button.
                      If the attribute is not specified, then the value equals to TEXT attribute.
                TEXT. [Optional] The text that is appended to input text field after the button is pressed.
                      If the attribute is not specified, then the value equals to NAME attribute.
                      At least NAME or TEXT attribute must be specified.
                TYPE. The type of the button:
                       - EQUALS. Indicate that after pressing this button the expression must be calculated.
                       - DIGIT. Indicate that this button is a digit or symbol which behavior equals to
                                digit behavior (e.g. point)
                       - BASE. Indicate that this button's appearance must equal to DIGIT appearance but
                               not it's behavior.
                       - SYMBOL. Indicates secondary operation
                                 (e.g. abs, pow or plus, multiplication (although they are not))
                LOCALE_EAST. Explanation requires an example. In western literature tangent's abbreviation
                             is tan, while in eastern - tg. So this attribute specifies id of a button
                             that should be used instead if the locale is eastern.
        -->
        <Button id="0" text="=" type="equals"/>
        <Button id="1" text="0" type="digit"/>
        <Button id="2" text="1" type="digit"/>
        <Button id="3" text="2" type="digit"/>
        <Button id="4" text="3" type="digit"/>
        <Button id="5" text="4" type="digit"/>
        <Button id="6" text="5" type="digit"/>
        <Button id="7" text="6" type="digit"/>
        <Button id="8" text="7" type="digit"/>
        <Button id="9" text="8" type="digit"/>
        <Button id="10" text="9" type="digit"/>
        <Button id="11" text="," type="symbol"/>
        <Button id="12" text="e" type="symbol" inverseOnLongClick="true"/>
        <Button id="13" text="&#x3c0;" type="symbol" inverseOnLongClick="true"/> <!-- PI -->
        <Button id="14" text="(" type="symbol"/>
        <Button id="15" text=")" type="symbol"/>
        <Button id="16" name="|x|" text="abs(" type="symbol"/>
        <Button id="17" text="+" type="symbol"/>
        <Button id="18" text="&#x2212;" type="symbol"/> <!-- minus -->
        <Button id="19" text="&#xd7;" type="symbol"/> <!-- multiply -->
        <Button id="20" text="&#xf7;" type="symbol"/> <!-- divide -->
        <Button id="21" text="!" type="symbol"/>
        <Button id="22" text="^" type="symbol"/>
        <Button id="23" text="&#x221a;" type="symbol"/> <!-- sqrt -->
        <Button id="24" text="." type="digit"/>
        <Button id="25" text="&#x221e;" type="symbol"/> <!-- infinity -->
        <Button id="26" text="NaN" type="symbol"/>
        <Button id="27" text="E" type="symbol" inverseOnLongClick="true"/>
        <Button id="28" text="%" type="symbol"/>
        <Button id="29" text="=" type="symbol"/>

        <Button id="30" name="sin" text="sin(" type="base"/>
        <Button id="31" name="cos" text="cos(" type="base"/>
        <Button id="32" name="tan" text="tan(" type="base" localeEast="36"/>
        <Button id="33" name="asin" text="asin(" type="base"/>
        <Button id="34" name="acos" text="acos(" type="base"/>
        <Button id="35" name="atan" text="atan(" type="base" localeEast="37"/>
        <Button id="36" name="tg" text="tg(" type="base"/>
        <Button id="37" name="atg" text="atg(" type="base"/>
        <Button id="38" name="log" text="log(" type="base"/>
        <Button id="39" name="lg" text="lg(" type="base"/>
        <Button id="40" name="ln" text="ln(" type="base"/>

        <Button id="41" name="" type="symbol"/>
        <Button id="42" name="a" type="base" inverseOnLongClick="true"/>
        <Button id="43" name="b" type="base" inverseOnLongClick="true"/>
        <Button id="44" name="c" type="base" inverseOnLongClick="true"/>
        <Button id="45" name="f" type="base" inverseOnLongClick="true"/>
        <Button id="46" name="g" type="base" inverseOnLongClick="true"/>
