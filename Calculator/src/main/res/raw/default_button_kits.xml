        <Button id="54" name="x" type="base" inverseOnLongClick="true"/>
        <Button id="55" name="y" type="base" inverseOnLongClick="true"/>
        <Button id="56" name="z" type="base" inverseOnLongClick="true"/>
        <Button id="57" name="α" type="base" inverseOnLongClick="true"/>
